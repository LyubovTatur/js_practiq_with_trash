<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.company.Place" table="place" schema="test1">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="namee">
            <column name="namee" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="placeDatetime">
            <column name="place_datetime" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>